{
  "name": "foreign-text-parser",
  "version": "0.0.1",
  "description": "A Simple text parser to find common words in selected text",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dom-waterson/foreign-text-parser.git"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "text",
    "parser",
    "foreign",
    "language",
    "words"
  ],
  "author": "dom-waterson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dom-waterson/foreign-text-parser/issues"
  },
  "homepage": "https://github.com/dom-waterson/foreign-text-parser#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
