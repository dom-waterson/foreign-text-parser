    "foreign",
    "language",
    "words"
  ],
  "author": "dom-waterson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dom-waterson/foreign-text-parser/issues"
  },
  "homepage": "https://github.com/dom-waterson/foreign-text-parser#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
