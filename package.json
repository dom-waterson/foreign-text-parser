{
  "name": "foreign-text-parser",
  "version": "0.0.3",
  "description": "A Simple text parser to find common words in selected text",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dom-waterson/foreign-text-parser.git"
  },
  "files": [
